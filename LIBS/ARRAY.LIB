;$SUB REDIM(ARRAY_PTR,NEW_SIZE)
V_REDIM:
	PUSH ebp
	MOV ebp, esp
	
	MOV ebx, DWORD [ebp + 12]
	INC DWORD [ebp + 8]
	SHL DWORD [ebp + 8], 2
	invoke HeapReAlloc,[hHeap],HEAP_ZERO_MEMORY,DWORD [ebx],DWORD [ebp + 8]
	
	MOV DWORD [ebx], eax
	MOV ebx, DWORD [ebp + 8]
	MOV DWORD [eax], ebx
	
	MOV esp, ebp
	POP ebp
	RET

